cy.get('body').then($body => {
  const $btn = $body.find('button[type="submit"]');
  if ($btn.length) {
    cy.wrap($btn).click({ force: true }); // or use $btn[0].click()
  } else {
    cy.log('🔁 Submit button not found, skipping click.');
  }
});



///////////////////////

cy.get('body').then($body => {
  const btn = $body.find('button[type="submit"]')[0];
  if (btn) btn.click(); // No retry, no wait, no Cypress errors
});
